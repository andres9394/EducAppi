<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.project.andre.educappi.Controlador.HerramientasOnline">

    <Button
        android:id="@+id/btnTool1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="abrirHerramienta"
        android:text="EverNote"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />

    <Button
        android:id="@+id/btnTool2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="abrirHerramienta"
        android:text="QuizBean"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="94dp" />

    <Button
        android:id="@+id/btnTool3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="abrirHerramienta"
        android:text="Duolingo" />

    <Button
        android:id="@+id/btnTool4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="abrirHerramienta"
        android:text="Socrative" />

    <Button
        android:id="@+id/btnTool5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="abrirHerramienta"
        android:text="CodeCademy" />

    <Button
        android:id="@+id/btnTool6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="abrirHerramienta"
        android:text="WolframAlpha" />
</LinearLayout>
